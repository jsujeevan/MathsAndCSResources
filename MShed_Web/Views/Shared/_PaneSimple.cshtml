@model MShed_Web.Models.Dispatching.PageModel
@{
    MShed_Web.Models.Dispatching.SimplePaneItem SimplePaneItem_o = (MShed_Web.Models.Dispatching.SimplePaneItem)Model.PaneItem;
}
<div class="g-pa-10">
    <div class="media g-mb-10">
        <div class="d-flex align-self-center">
            <h1 class="g-font-weight-300 g-font-size-28 g-color-black mt-0 mb-0">@SimplePaneItem_o.PageHeading</h1>
        </div>
    </div>
    @RenderBody()
</div>

<script>
    $("#breadcrumbNav").append('<li class="list-inline-item g-mr-10"><a class="u-link-v5 g-color-gray-dark-v6 g-color-secondary--hover g-valign-middle" href="@Request.Url.AbsoluteUri">@SimplePaneItem_o.PageTitle</a><i class="hs-admin-angle-right g-font-size-12 g-color-gray-light-v6 g-valign-middle g-ml-10"></i></li>');
</script>